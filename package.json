{
    "name": "@bearei/element",
    "version": "0.0.1",
    "main": "lib/commonjs/index.js",
    "react-native": "src/index.tsx",
    "module": "lib/module/index.js",
    "types": "lib/typescript/src/index.d.ts",
    "author": "bearei <bear_ei@outlook.com>",
    "description": "bear-ei project element",
    "license": "MIT",
    "readmeFilename": "README.md",
    "keywords": [
        "react",
        "react native",
        "typescript",
        "material design"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/bearei-team/element"
    },
    "bugs": {
        "url": "https://github.com/bearei-team/element/issues"
    },
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "lint": "eslint .",
        "start": "react-native start",
        "test": "jest",
        "update-stories": "sb-rn-get-stories --config-path .ondevice",
        "prestart": "yarn update-stories",
        "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "build": "bob build",
        "clean": "shx rm -rf lib",
        "publish": "yarn clean && yarn build && yarn npm publish --access public"
    },
    "dependencies": {
        "@bearei/theme": "^1.3.11",
        "@material-design-icons/svg": "latest",
        "async-validator": "^4.2.5",
        "immer": "^10.0.3",
        "react": "^18.2.0",
        "react-native": "^0.72.5",
        "react-native-svg": "^14.0.0",
        "styled-components": "6.0.7",
        "use-immer": "^0.9.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@react-native-async-storage/async-storage": "^1.19.3",
        "@react-native-community/datetimepicker": "^7.6.0",
        "@react-native-community/slider": "^4.4.3",
        "@react-native/eslint-config": "^0.72.2",
        "@react-native/metro-config": "^0.72.11",
        "@storybook/addon-actions": "^6.5.13",
        "@storybook/addon-controls": "^6.5.13",
        "@storybook/addon-essentials": "^6.5.13",
        "@storybook/addon-links": "^6.5.13",
        "@storybook/addon-ondevice-actions": "^6.5.1",
        "@storybook/addon-ondevice-backgrounds": "^6.5.1",
        "@storybook/addon-ondevice-controls": "^6.5.1",
        "@storybook/addon-ondevice-notes": "^6.5.1",
        "@storybook/addon-react-native-web": "^0.0.21",
        "@storybook/builder-webpack5": "^6.5.16",
        "@storybook/manager-webpack5": "^6.5.16",
        "@storybook/react": "^6.5.13",
        "@storybook/react-native": "^6.5.6",
        "@svgr/webpack": "^8.1.0",
        "@tsconfig/react-native": "^3.0.0",
        "@types/react": "^18.0.24",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.2.1",
        "babel-loader": "^8.3.0",
        "babel-plugin-react-native-web": "^0.19.9",
        "eslint": "^8.19.0",
        "jest": "^29.2.1",
        "metro-react-native-babel-preset": "^0.76.8",
        "prettier": "^2.4.1",
        "react-dom": "^18.2.0",
        "react-native-builder-bob": "^0.23.1",
        "react-native-safe-area-context": "^4.7.2",
        "react-native-svg-transformer": "^1.1.0",
        "react-native-web": "^0.19.9",
        "react-test-renderer": "^18.2.0",
        "shx": "^0.3.4",
        "storybook": "^7.5.3",
        "typescript": "^4.8.4",
        "webpack": "^5.88.2"
    },
    "engines": {
        "node": ">=16"
    },
    "packageManager": "yarn@3.6.4",
    "source": "src/index.tsx",
    "files": [
        "src",
        "lib",
        "!**/__tests__",
        "!**/__fixtures__",
        "!**/__mocks__",
        "!**/.ondevice",
        "!**/*.map"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "exclude": "**/*.stories.{js,ts,tsx}",
        "targets": [
            [
                "commonjs",
                {
                    "sourceMaps": false
                }
            ],
            [
                "module",
                {
                    "sourceMaps": false
                }
            ],
            [
                "typescript",
                {
                    "sourceMaps": false
                }
            ]
        ]
    },
    "eslintIgnore": [
        "node_modules/",
        "lib/"
    ]
}
